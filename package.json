{
  "name": "conjure-client",
  "version": "0.2.0",
  "description": "An HTTP bridge library for use in front end applications and generated conjure code",
  "sideEffects": false,
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm-run-all clean compile",
    "circle-publish": "./scripts/circle-publish-npm",
    "clean": "rm -rf lib/*",
    "compile": "tsc -p ./src",
    "lint": "tslint 'src/**/*.ts*' -t codeFrame",
    "lint-fix": "yarn lint --fix",
    "test": "jest",
    "verify": "npm-run-all -p test lint"
  },
  "dependencies": {
    "uuidjs": "^4.0.3"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "^1.3.1",
    "@types/express": "^4.11.1",
    "@types/fetch-mock": "^5.12.1",
    "@types/jest": "^21.1.5",
    "es6-shim": "^0.35.3",
    "express": "^4.16.2",
    "fetch-mock": "^5.13.1",
    "jest": "^21.2.1",
    "node-fetch-polyfill": "^2.0.6",
    "npm-run-all": "^4.1.1",
    "ts-jest": "^21.1.4",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "~2.8.3",
    "whatwg-fetch": "^2.0.3"
  },
  "author": "Palantir Technologies, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:palantir/conjure-typescript-client.git"
  }
}
